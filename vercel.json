{
    "builds": [
      {
        "src": "task_qtec/wsgi.py",
        "use": "@vercel/python",
        "config":{
          "maxLamdaSize": "15mb",
          "runtime":"python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "task_qtec/wsgi.py"
      }
    ]
}